(define (problem dress-up-gone-wrong)
  (:domain dress-up)
  (:objects
    sara ben monster - character
    red-dress hat blue-shirt tie - object)
  (:init
    (at red-dress sara)
    (at hat sara)
    (intends sara (dressed-up sara))
    (alive sara)
    (intends sara (alive sara))
    (at blue-shirt ben)
    (at tie ben)
    (intends ben (dressed-up ben))
    (alive ben)
    (intends ben (alive ben))
    (intends sara (holding-hands))
    (intends ben (holding-hands))
    (intends sara (kissing))
    (intends ben (kissing))
    (alive monster)
    (intends monster (alive monster))
    (intends monster (monster monster))
    (not (in-wardrobe sara))
    (not (in-wardrobe ben))
    (not (in-wardrobe monster)))
  (:goal
    (and (in-wardrobe sara)
    (in-wardrobe ben)))