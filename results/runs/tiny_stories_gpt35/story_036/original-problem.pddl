(define (problem pin-rescue)
  (:domain pin-rescue)
  (:objects
    pin1 pin2 pin3 pin4 - pin)
  (:init
    (in-box pin1)
    (in-box pin2)
    (in-box pin3)
    (in-box pin4)
    (intends fairy (alive fairy))
    (intends fairy (alive pin1))
    (intends fairy (alive pin2))
    (intends fairy (alive pin3))
    (intends fairy (alive pin4))
    (not (alive fairy))
    (not (alive pin1))
    (not (alive pin2))
    (not (alive pin3))
    (not (alive pin4))
    (on-floor pin1)
    (on-floor pin2)
    (on-floor pin3)
    (on-floor pin4))
  (:goal
    (and (in-box pin1)
    (in-box pin2)
    (in-box pin3)
    (in-box pin4)
    (alive fairy)
    (alive pin1)
    (alive pin2)
    (alive pin3)
    (alive pin4)))
)